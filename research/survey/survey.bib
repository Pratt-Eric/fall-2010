@inproceedings{staring_people,
Author = {Dewei Chen and Jie Tang and Juanzi Li and Lizhu Zhou},
Title = {Discovering the Staring People From Social Networks},
Booktitle = {18th International World Wide Web Conference (WWW2009)},
Year = {2009},
Month = {April},
Abstract = {In this paper, we study a novel problem of staring people dis- 
covery from social networks, which is concerned with finding 
people who are not only authoritative but also sociable in the 
social network. We formalize this problem as an optimiza- 
tion programming problem. Taking the co-author network 
as a case study, we define three objective functions and pro- 
pose two methods to combine these objective functions. A 
genetic algorithm based method is further presented to solve 
this problem. Experimental results show that the proposed 
solution can effectively find the staring people from social 
networks.},
Url = {http://data.semanticweb.org/conference/www/2009/paper/190}
}


@article{journal/structure_stream,
	location = {http://www.scientificcommons.org/43202914},
	title = {Bursty and Hierarchical Structure in Streams},
	author = {Jon Kleinbe Rg and Jon Kleinberg},
	year = {2002},
	abstract = {A fundamental problem in text data mining is to extract meaningful structure from document streams that arrive continuously over time. E-mail and news articles are two natural examples of such streams, each characterized by topics that appear, grow in intensity for a period of time, and then fade away. The published literature in a particular research field can be seen to exhibit similar phenomena over a much longer time scale. Underlying much of the text mining work in this area is the following intuitive premise --- that the appearance of a topic in a document stream is signaled by a &quot;burst of activity,&quot; with certain features rising sharply in frequency as the topic emerges.},
	publisher = {ACM Press},
	url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.59.457},
